{
  "author": "planttheidea",
  "ava": {
    "concurrency": 5,
    "babel": "inherit",
    "failFast": true,
    "files": [
      "!test/helpers/*.js",
      "test/*.js"
    ],
    "require": [
      "babel-register",
      "test/helpers/setup-browser-env.js"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/react-pure-lifecycle/issues"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.3.0"
  },
  "description": "Add pure function lifecycle methods to any React component",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-rapid7": "^2.9.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "html-webpack-plugin": "^2.28.0",
    "in-publish": "2.0.0",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "json-loader": "0.5.4",
    "lodash-webpack-plugin": "^0.11.2",
    "nyc": "^10.2.0",
    "optimize-js-plugin": "^0.0.4",
    "prop-types": "^15.5.8",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.4"
  },
  "homepage": "https://github.com/planttheidea/react-pure-lifecycle#readme",
  "keywords": [
    "react",
    "lifecycle",
    "functional"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "react-pure-lifecycle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/react-pure-lifecycle.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "clean": "rimraf lib && rimraf dist",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || echo ''",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run clean && npm run transpile && npm run build && npm run build:minified",
    "start": "NODE_ENV=development webpack-dashboard -- webpack-dev-server --config=webpack.config.dev.js",
    "test": "NODE_PATH=.:src NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_PATH=.:src NODE_ENV=test ava --watch",
    "transpile": "babel src --out-dir lib --copy-files"
  },
  "version": "2.0.0"
}
